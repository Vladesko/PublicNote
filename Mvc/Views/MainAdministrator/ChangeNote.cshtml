    Layout = "_Layout";
    ViewBag.Title = "Chanage Note";
}
<nav class="navbar navbar-dark bg-dark">
    <div class="container-fluid">
        <a class="navbar-brand a-style" asp-controller="MainAdministrator" asp-action="ChangeText" asp-route-id="@Model">Change text</a>
